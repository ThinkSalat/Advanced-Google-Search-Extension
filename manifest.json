{
  "manifest_version": 3,
  "name": "Advanced Google Search",
  "version": "1.0",
  "description": "Makes the advanced search capabilities easily available on the toolbar",
  "icons": {
    "128": "images/icon-128.png",
    "48": "images/icon-48.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "*://*.google.com/search?*"
      ]
    }
  ]
}